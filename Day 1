TASK 1

with open('data.txt', 'r') as file:
    lines = file.readlines()

totalSum = 0

for line in lines:
    numbers = [int(char) for char in line if char.isdigit()]

    if len(numbers) == 1:
        firstLastNumbers = [numbers[0], numbers[0]]
    else:
        firstLastNumbers = [numbers[0], numbers[-1]]

    twoDigitNmb = int(''.join(map(str, firstLastNumbers)))

    totalSum += twoDigitNmb

print(totalSum)
